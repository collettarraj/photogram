<%= form_with(model: @usersbest, local: true) do |f| %>

  <div class="form-group">
    <%= f.label :dishes_id %>

    <%= f.collection_select :dishes_id, Dish.all, :id, :to_s, {}, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :venue_id %>

    <%= f.collection_select :venue_id, Venue.all, :id, :to_s, {}, class: "form-control" %>
  </div>

  <%= f.hidden_field :users_id, value: @user.id %>

  <%= f.submit "Create usersbest", class: "btn btn-block btn-outline-secondary" %>
<% end %>
